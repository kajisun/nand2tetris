/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=si);
    And(a=a, b=si, out=asi);
    And(a=b, b=sel, out=bs);
    Or(a=asi, b=bs, out=out);
}
